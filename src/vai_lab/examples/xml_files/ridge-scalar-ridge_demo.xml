<pipeline>
    <Initialiser name="Initialiser">
        <relationships>
            <child name="Modelling" />
        </relationships>
        <coordinates>
            [(350.0,50),0,{}]
        </coordinates>
        <inputdata>
            <X file="./examples/supervised_regression/X_tr.csv" />
            <Y file="./examples/supervised_regression/Y_tr.csv" />
            <X_test file="./examples/supervised_regression/X_tst.csv" />
            <Y_test file="./examples/supervised_regression/Y_tst.csv" />
        </inputdata>
    </Initialiser>
    <Modelling name="Modelling">
        <relationships>
            <parent name="Initialiser" />
            <child name="Data Processing" />
        </relationships>
        <coordinates>
            [(350,180),2,{0:'d0-u2'}]
        </coordinates>
        <plugin type="RidgeRegression">
            <alpha>
                 0.01
            </alpha>
        </plugin>
        <inputdata>
            <X module="Initialiser" />
        </inputdata>
    </Modelling>
    <DataProcessing name="Data Processing">
        <relationships>
            <parent name="Modelling" />
            <child name="Modelling-1" />
        </relationships>
        <coordinates>
            [(350.0,350.0),3,{2:'d2-u3'}]
        </coordinates>
        <inputdata>
            <X module="Modelling" />
        </inputdata>
        <plugin type="StandardScaler">
            <Data>
                 X
            </Data>
        </plugin>
    </DataProcessing>
    <Modelling name="Modelling-1">
        <relationships>
            <parent name="Data Processing" />
            <child name="Output" />
        </relationships>
        <coordinates>
            [(350,408),4,{3:'d3-u4'}]
        </coordinates>
        <inputdata>
            <X module="Data Processing" />
        </inputdata>
        <plugin type="RidgeRegression">
            <alpha>
                 0.01
            </alpha>
        </plugin>
    </Modelling>
    <Output name="Output">
        <relationships>
            <parent name="Modelling-1" />
        </relationships>
        <coordinates>
            [(350.0,650),1,{4:'d4-u1'}]
        </coordinates>
        <plugin type="Output">
            <outdata>
                Modelling
                Modelling-1
            </outdata>
            <outpath>
                 .\examples\results\output.pkl
            </outpath>
        </plugin>
    </Output>
</pipeline>
