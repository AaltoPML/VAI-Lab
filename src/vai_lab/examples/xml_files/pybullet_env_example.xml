<pipeline>
    <Initialiser name="Initialiser">
        <relationships>
            <child name="MyEnv" />
        </relationships>
        <coordinates>
            [(350.0,50),0,{}]
        </coordinates>
    </Initialiser>
    <Environment name="MyEnv">
        <relationships>
            <parent name="Initialiser" />
            <child name="Output" />
        </relationships>
        <coordinates>
            [(350.0,350.0),2,{0:'d0-u2'}]
        </coordinates>
        <plugin type="PyBullet">
            <model_dir>
                 plane.urdf
                 ./Environment/resources/models/half_cheetah_with_mass.xml
            </model_dir>
            <usegui>
                 False
            </usegui>
            <setGravity>
                    0.0
                    0.0
                    -9.81
            </setGravity>
            <timestep>
                    0.01
            </timestep>
            <max_steps>
                    10
            </max_steps>            
        </plugin>
        <inputdata>
            <X module="Initialiser" />
        </inputdata>
    </Environment>
    <Output name="Output">
        <plugin type="Output">
            <outdata>
                MyEnv
            </outdata>
            <outpath>
                 .\examples\results\output.pkl
            </outpath>
        </plugin>
        <relationships>
            <parent name="MyEnv" />
        </relationships>
        <coordinates>
            [(350.0,650),1,{2:'d2-u1'}]
        </coordinates>
    </Output>
</pipeline>