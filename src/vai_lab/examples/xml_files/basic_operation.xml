<pipeline>
    <Initialiser name="Init">
        <inputdata>
            <X file="./examples/image_classification/training_images/" />
            <Y file="./examples/image_classification/Y_data.csv" />
        </inputdata>
        <relationships>
            <child name="My First UserInteraction Module" />
        </relationships>
    </Initialiser>
    <loop type="for" condition="2" name="For Loop 1">
        <UserInteraction name="My First UserInteraction Module">
            <plugin type="manual">
            </plugin>
            <relationships>
                <parent name="Init" />
                <child name="Data Processing 1" />
            </relationships>
        </UserInteraction>
        <loop type="for" condition="1" name="For Loop 2">
            <UserInteraction name="My Second UserInteraction Module">
                <plugin type="canvas">
                    <class_list>
                        ['State_a','Action_a']
                        ['State_x','State_y','Action_x','Action_y']
                        ['State_x','State_y','Action_x','Action_y']
                    </class_list>
                </plugin>
                <relationships>
                    <parent name="Data Processing 1" />
                    <child name="Data Processing 1" />
                    <child name="Output" />
                </relationships>
            </UserInteraction>
        </loop>
    </loop>
    <Output>
        <plugin type="">
            <out_data>
                all
            </out_data>
            <save_to file="../resources/output_test.csv" />
        </plugin>
        <relationships>
            <parent name="My Second UserInteraction Module" />
        </relationships>
    </Output>
</pipeline>
