<pipeline>
    <Initialiser name="Initialiser">
        <relationships>
            <child name="My Data Processing" />
        </relationships>
		<inputdata>
            <X file="./supervised_classification/X_tr.csv"/>
            <Y file="./supervised_classification/Y_tr.csv"/>
            <X_test file="./supervised_classification/X_tst.csv"/>
            <Y_test file="./supervised_classification/Y_tst.csv"/>
        </inputdata>
    </Initialiser>
  
    <DataProcessing name="My Data Processing">
        <relationships>
            <parent name="Initialiser" />
            <child name="My Modelling Module" />
        </relationships>
        <plugin type="StandardScaler">
            <Data>
                X
            </Data>
        </plugin>
    </DataProcessing>
  
    <Modelling name="My Modelling Module">
        <relationships>
            <parent name="My Data Processing" />
            <child name="Output" />
        </relationships>
        <plugin type="RandomForestClassifier" />
    </Modelling>
  
    <Output name="Output">
        <relationships>
            <parent name="My Modelling Module" />
        </relationships>
      <outData file="./my_results.csv" />
    </Output>
  
</pipeline>
