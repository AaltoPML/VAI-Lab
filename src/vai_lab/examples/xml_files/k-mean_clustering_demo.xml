<pipeline>
    <Initialiser name="Initialiser">
        <relationships>
            <child name="Data Processing" />
        </relationships>
        <coordinates>
            [(300.0,50),0,{}]
        </coordinates>
        <inputdata>
            <X file="./examples/supervised_regression/X_tr.csv" />
            <X_test file="./examples/supervised_regression/X_tst.csv" />
        </inputdata>
    </Initialiser>
    <DataProcessing name="Data Processing">
        <relationships>
            <parent name="Initialiser" />
            <child name="Modelling" />
        </relationships>
        <coordinates>
            [(301,201),2,{0:'d0-u2'}]
        </coordinates>
        <plugin type="StandardScaler">
            <Data>
                X
            </Data>
        </plugin>
    </DataProcessing>
    <Modelling name="Modelling">
        <relationships>
            <parent name="Data Processing" />
            <child name="Output" />
        </relationships>
        <coordinates>
            [(300.0,300.0),3,{2:'d2-u3'}]
        </coordinates>
        <plugin type="KMeans" />
    </Modelling>
    <Output name="Output">
        <plugin type="" />
        <relationships>
            <parent name="Modelling" />
        </relationships>
        <coordinates>
            [(300.0,550),1,{3:'d3-u1'}]
        </coordinates>
    </Output>
</pipeline>
