<Settings>
    <pipeline>
        <Initialiser name="Init">
            <initial_data file="../resources/intial_data_test.csv" />
            <goal file="../resources/design_goals_test.csv" />&gt;
            <relationships>
                <child name="My First GUI Module" />
            </relationships>
        </Initialiser>
        <GUI name="My First GUI Module">
            <plugin type="manual">
                <class_list>
                                        Atelectasis
                                        Cardiomelagy
                                        Effusion
                                        Infiltration
                                        Mass
                                        Nodule
                                        Pneumonia
                                        Pneumothorax
                                    
                </class_list>
            </plugin>
            <relationships>
                <parent name="Init" />
                <child name="Data Processing 1" />
            </relationships>
        </GUI>
        <loop type="for" condition="10" name="For Loop 1">
            <DataProcessing name="Data Processing 1">
                <plugin type="interpolation">
                    <interpolation_resolution value="10" />
                </plugin>
                <relationships>
                    <parent name="My First GUI Module" />
                    <parent name="My Second GUI Module" />
                    <child name="My Second GUI Module" />
                </relationships>
            </DataProcessing>
            <GUI name="My Second GUI Module">
                <plugin type="canvas">
                    <class_list>
                                                ['State_a','Action_a']
                                                ['State_x','State_y','Action_x','Action_y']
                                                ['State_x','State_y','Action_x','Action_y']
                                            
                    </class_list>
                </plugin>
                <relationships>
                    <parent name="Data Processing 1" />
                    <child name="Data Processing 1" />
                    <child name="Output" />
                </relationships>
            </GUI>
            <loop type="while" condition="a= b" name="While Loop">
                <DataProcessing name="Data Processing 2">
                    <plugin type="interpolation">
                        <interpolation_resolution value="10" />
                    </plugin>
                    <relationships>
                        <parent name="My First GUI Module" />
                        <parent name="My Second GUI Module" />
                        <child name="My Second GUI Module" />
                    </relationships>
                </DataProcessing>
                <GUI name="My Third GUI Module">
                    <plugin type="canvas">
                        <class_list>
                                                        ['State_a','Action_a']
                                                        ['State_x','State_y','Action_x','Action_y']
                                                        ['State_x','State_y','Action_x','Action_y']
                                                    
                        </class_list>
                    </plugin>
                    <relationships>
                        <parent name="Data Processing 1" />
                        <child name="Data Processing 1" />
                        <child name="Output" />
                    </relationships>
                </GUI>
            </loop>
        </loop>
        <Output>
            <out_data>
                                all
                            
            </out_data>
            <save_to file="../resources/output_test.csv" />
            <relationships>
                <parent name="My Second GUI Module" />
            </relationships>
        </Output>
        <loop type="for" condition="10" name="my_loop_3">
            <relationships>
                <parent name="Init" />
            </relationships>
        </loop>
        <GUI name="added_mod">
            <plugin type="startpage">
                <class_list>
                                        test_1
                                        test_2
                                    
                </class_list>
                <class_list_2>
                                        test_1
                                        test_2
                                    
                </class_list_2>
            </plugin>
            <relationships>
                <parent name="For Loop 1" />
                <child name="Output" />
            </relationships>
            <coordinates>
                                                [2, 3, 4, 5]
                                            
                            
            </coordinates>
        </GUI>
        <loop type="for" condition="10" name="my_loop_3">
            <relationships>
                <parent name="Init" />
            </relationships>
        </loop>
        <GUI name="added_mod">
            <plugin type="startpage">
                <class_list>
                    test_1
                    test_2
                </class_list>
                <class_list_2>
                    test_1
                    test_2
                </class_list_2>
            </plugin>
            <relationships>
                <parent name="For Loop 1" />
                <child name="Output" />
            </relationships>
            <coordinates>
                [2, 3, 4, 5]
            </coordinates>
        </GUI>
    </pipeline>
    <datastructure name="datatest">
        <data_fields>
                        State_0
                        State_1
                        Action
                        Reward
                        Termination
                        Goal
                    
        </data_fields>
    </datastructure>
</Settings>
