                    0.0
                    0.0
                    -9.81
            </setGravity>
            <setGravit3y>
                    [0.0,0.0,-9.81]
            </setGravit3y>
            <!-- <setPhysicsEngineParameter>
                <fixedTimeStep>
                    0.01
                </fixedTimeStep>
                <numSolverIterations>
                    1000
                </numSolverIterations>
            </setPhysicsEngineParameter> -->
            
        </plugin>
    </Environment>
    <Output name="Output">
        <plugin type="" />
        <relationships>
            <parent name="Modelling" />
        </relationships>
        <coordinates>
            [(300.0,550),1,{2:'d2-u1'}]
        </coordinates>
    </Output>
