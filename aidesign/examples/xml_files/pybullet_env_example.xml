            <child name="Output" />
        </relationships>
        <coordinates>
            [(300.0,300.0),2,{0:'d0-u2'}]
        </coordinates>
        <plugin type="PyBullet">
            <!-- <model_dir>
                 plane.urdf
                 ./Environment/resources/models/half_cheetah_with_mass.xml
            </model_dir>
            <headless>
                 False
            </headless> -->
