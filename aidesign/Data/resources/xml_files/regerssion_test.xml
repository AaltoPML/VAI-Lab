                 c
            </target_name>
        </plugin>
    </Modelling>
    <Output name="Output">
        <plugin type="" />
        <relationships>
            <parent name="Modelling" />
        </relationships>
        <coordinates>
            [(300.0,550),1,{2:'d2-u1'}]
        </coordinates>
    </Output>
