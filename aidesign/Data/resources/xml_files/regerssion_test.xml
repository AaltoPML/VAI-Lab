<pipeline>
    <Initialiser name="Initialiser">
        <relationships>
            <child name="Modelling" />
        </relationships>
        <coordinates>
            [(300.0,50),0,{}]
        </coordinates>
        <inputdata>
            <X file="./Data/resources/supervised_regression/1/x_train.csv" />
            <Y file="./Data/resources/supervised_regression/1/y_train.csv" />
        </inputdata>
    </Initialiser>
    <Modelling name="Modelling">
        <relationships>
            <parent name="Initialiser" />
